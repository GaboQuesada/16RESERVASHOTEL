<odoo>
    <!--Form view for folio report wizard -->
    <record id="view_showhabitaciones_wizard" model="ir.ui.view">
        <field name="name">view.showhabitaciones.wizard</field>
        <field name="model">showhabitaciones</field>
        <field name="arch" type="xml">
            <form string="Agregar lineas">
              
              
                   
                    <field name="lineascamas_id">
                            <tree>
                                <field name="product_id" string="Nombre"/>
                                <field name="date_1" />
                                <field name="date_2" />
                                <field name="date_3" />
                                <field name="date_4" />
                                <field name="date_5" />
                                <field name="date_6" />
                                <field name="date_7" />
                                <field name="date_8" />
                                <field name="date_9" />
                                <field name="date_10" />
                                <field name="date_11" />
                                <field name="date_12" />
                                <field name="date_13" />
                                <field name="date_14" />
                                <field name="date_15" />
                                <field name="date_16" />
                                <field name="date_17" />
                                <field name="date_18" />
                                <field name="date_19" />
                                <field name="date_20" />
                                <field name="date_21" />
                                <field name="date_22" />
                                <field name="date_23" />
                                <field name="date_24" />
                                <field name="date_25" />
                                <field name="date_26" />
                                <field name="date_27" />
                                <field name="date_28" />
                                <field name="date_29" />
                                <field name="date_30" />
                                <field name="date_31" />
                                
                            </tree>
                    </field>

                <footer>
                    
                    <button
                        string="Cancelar"
                        icon="fa-close"
                        class="btn btn-primary"
                        special="cancel"
                    />
                </footer>
            </form>
        </field>
    </record>
    <!--Action for folio report wizard -->
    <record id="actions_showhabitaciones_wizard" model="ir.actions.act_window">
        <field name="name">actions showhabitaciones wizard</field>
        <field name="res_model">showhabitaciones</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>


</odoo>